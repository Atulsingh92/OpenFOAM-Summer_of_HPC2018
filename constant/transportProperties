FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);


sigma            0.07;


water
{
    transportModel Newtonian;
   
    nu              1.2024e-06;
    rho              998;
}


air
{
    transportModel Newtonian;

    nu              1.4496e-05;
    rho             1.19;
}



